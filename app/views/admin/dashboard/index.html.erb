<h1 align = "center">Admin Dashboard</h1>
<section class = "grid-container">
  <% @orders.each do |order|%>
    <section class = "grid-item">
      <h2><%= "Order: #{order.id}" %></h2>
      <p><%="User: #{order.user.name}"%></p>
      <p><%="Created on: #{order.created_at}"%></p>

    </section>
  <% end %>
</section>
